{
  "Product": {
    "allOf": [
      { "$ref": "product.json" },
      {
        "properties": {
          "stocks": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "IDW": { "type": "integer" },
                "quantity": { "type": "integer" },
                "location": { "type": "string" }
              },
              "required": ["IDW", "quantity", "location"]
            }
          }
        }
      }
    ]
  },
  "Warehouse": { "$ref": "db1.json#/Warehouse" },
  "OrderLine": { "$ref": "db1.json#/OrderLine" },
  "Client": { "$ref": "db1.json#/Client" }
}
